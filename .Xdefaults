! URxvt*reverseVideo: True
! URxvt*scrollBar: False

URxvt*transparent: false
URxvt*shading: 20
URxvt*background: #1d2021
URxvt*foreground: #d5c4a1
URxvt*font: xft:Ubuntu Mono:size=10,style=regular
URxvt*boldFont: xft:Ubuntu Mono:bold:size=10,style=bold
URxvt*saveLines: 12000
URxvt*scrollBar: false
URxvt*scrollstyle: rxvt
URxvt*perl-ext-common: default,clipboard,url-select,keyboard-select
URxvt.iso14755: false
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt*urlLauncher: /usr/bin/xdg-open
URxvt*matcher.button: 1
URxvt*metaSendsEscape: true
URxvt*colorUL: #86a2be
! black
URxvt*color0  : #2E3436
URxvt*color8  : #555753
! red
URxvt*color1  : #CC241D
URxvt*color9  : #FB4934
! green
URxvt*color2  : #98971A
URxvt*color10 : #B8BB26
! yellow
URxvt*color3  : #D79921
URxvt*color11 : #FABD2f
! blue
URxvt*color4  : #458588
URxvt*color12 : #83A598
! magenta
URxvt*color5  : #B16286
URxvt*color13 : #D3869B
! cyan
URxvt*color6  : #689D6A
URxvt*color14 : #8EC07C
! white
URxvt*color7  : #A89984
URxvt*color15 : #EBDBB2

! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
rofi.color-enabled: true
rofi.color-window: #002b37, #002b37, #003642
rofi.color-normal: #002b37, #819396, #002b37, #003642, #819396
rofi.color-active: #002b37, #008ed4, #002b37, #003642, #008ed4
rofi.color-urgent: #002b37, #da4281, #002b37, #003642, #da4281
rofi.fake-transparency: true

! HiDPI Settings
! 96 is the default (100%)
! 120:125% | 144:150% : 192:200%
Xft.dpi: 96
